<h1 class="Title">התחברות</h1>
<button id="login-btn">
    <p id="login-btn-txt">
        התחבר עם WCA
    </p>
</button>

<!-- login btn onclick -->
<script>
    const loginBtn = document.querySelector("#login-btn");
    loginBtn.onclick = () => {
        loginBtn.disabled = true;
        window.location.href = "<%= auth_path %>";
    }
</script>